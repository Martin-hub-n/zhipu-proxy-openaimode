{
  "version": 2,
  "builds": [
    {
      "src": "zhipu.py", 
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.10"
      }
    }
  ],
  "routes": [
    {
      "src": "/v1/chat/completions",
      "dest": "zhipu.py"
    },
    {
      "src": "/v1/models",
      "dest": "zhipu.py"
    },
    {
      "src": "/(.*)",
      "dest": "zhipu.py"
    }
  ]
}
